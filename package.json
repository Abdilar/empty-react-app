  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@craco/craco": "^6.1.2",
    "@sentry/react": "^6.17.3",
    "@sentry/tracing": "^6.17.3",
    "@testing-library/jest-dom": "^5.16.1",
